CREATE TABLE `file` (
	`id` BIGINT(20) NOT NULL AUTO_INCREMENT,
	`telegram_id` BIGINT(20) NOT NULL DEFAULT '0',
	`file_name` VARCHAR(300) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`file_id` TEXT NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	`save` TINYINT(1) NULL DEFAULT '0',
	`message_id` BIGINT(20) NULL DEFAULT NULL,
	`type` VARCHAR(300) NULL DEFAULT NULL COLLATE 'utf8mb4_general_ci',
	PRIMARY KEY (`id`) USING BTREE
)
COLLATE='utf8mb4_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=2
;
